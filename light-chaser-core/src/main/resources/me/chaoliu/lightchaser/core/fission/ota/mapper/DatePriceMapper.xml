<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="me.chaoliu.lightchaser.core.fission.ota.mapper.DatePriceMapper">
    <select id="selectDatePriceByID" parameterType="int" resultType="DatePrice">
        select * from date_price where id = #{id}
    </select>

    <!-- 对应ProductMapper中的insertProduct方法  -->
    <insert id="insertDatePrice" parameterType="DatePrice">
        insert into date_price(`date`, price) values(#{date}, #{price})
    </insert>
</mapper>